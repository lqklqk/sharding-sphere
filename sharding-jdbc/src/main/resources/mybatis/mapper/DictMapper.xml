<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lqk.shardingjdbc.dao.mapper.DictMapper">
    <resultMap id="BaseResultMap" type="com.lqk.shardingjdbc.dao.entity.Dict">
        <result column="dict_id" property="dictId" jdbcType="BIGINT"/>
        <result column="dict_key" property="dictKey" jdbcType="VARCHAR"/>
        <result column="dict_val" property="dictVal" jdbcType="VARCHAR"/>
    </resultMap>
    <sql id="Base_Column">
        dict_id, dict_key, dict_val
    </sql>
    <insert id="insert">
        insert into dict (dict_key, dict_val)
        values (#{dictKey}, #{dictVal})
    </insert>
    <select id="selectByDictKey" resultMap="BaseResultMap">
        select <include refid="Base_Column"/>
        from dict where dict_key = #{dictKey}
    </select>
</mapper>